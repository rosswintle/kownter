        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.20",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.32.5",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    }
